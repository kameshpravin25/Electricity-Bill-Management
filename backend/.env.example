# ORACLE_LIB_DIR can point to the instantclient folder (contains oci.dll)
# or to the parent folder which contains instantclient_* directories
ORACLE_LIB_DIR=C:\path\to\instantclient_19_28

DB_USER=your_db_user
DB_PASSWORD=your_db_password
